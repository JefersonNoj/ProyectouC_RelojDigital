;----- SUBRUTINAS DE CONFIGURACIÓN -----
config_clk:
    BANKSEL OSCCON
    BCF ((OSCCON) and 07Fh), 6 ; IRCF/011/500 kHz (frecuencia de oscilación)
    BSF ((OSCCON) and 07Fh), 5
    BSF ((OSCCON) and 07Fh), 4
    BSF ((OSCCON) and 07Fh), 0 ; Reloj interno
    RETURN

config_io:
    BANKSEL ANSEL
    CLRF ANSEL ; I/O digitales
    CLRF ANSELH
    BANKSEL TRISA
    CLRF TRISA ; PORTA como salida
    BSF TRISB, BMODO ; ((PORTB) and 07Fh), 0 como entrada (boton para cambio de modo)
    BSF TRISB, UP ; ((PORTB) and 07Fh), 1 como entrada (boton para incremento)
    BSF TRISB, DOWN ; ((PORTB) and 07Fh), 2 como entrada (boton decremento)
    BSF TRISB, EDITAR ; ((PORTB) and 07Fh), 3 como entrada (boton para activiar/desactivar configuración manual)
    BSF TRISB, INICIO ; ((PORTB) and 07Fh), 4 como entrada (boton para iniciar/detener)
    CLRF TRISC ; PORTC como salida
    CLRF TRISD ; PORTD como salida
    CLRF TRISE ; PORTE como salida
    BCF OPTION_REG, 7 ; Habilitar resistencias pull-up para PORTB
    BSF WPUB, BMODO ; Habilitar pull-up para ((PORTB) and 07Fh), 0
    BSF WPUB, UP ; Habilitar pull-up para ((PORTB) and 07Fh), 1
    BSF WPUB, DOWN ; Habilitar pull-up para ((PORTB) and 07Fh), 2
    BSF WPUB, EDITAR ; Habilitar pull-up para ((PORTB) and 07Fh), 3
    BSF WPUB, INICIO ; Habilitar pull-up para ((PORTB) and 07Fh), 4
    BANKSEL PORTA
    CLRF PORTA ; Limpiar PORTA
    CLRF PORTC ; Limpiar PORTD
    CLRF PORTD ; Limpiar PORTC
    CLRF PORTE ; Limpiar PORTE
    RETURN

config_tmr0:
    BANKSEL OPTION_REG
    BCF ((OPTION_REG) and 07Fh), 5 ; Configurar reloj interno
    BCF ((OPTION_REG) and 07Fh), 3 ; Asignar prescaler al TMR0
    BSF ((OPTION_REG) and 07Fh), 2 ; Prescaler/110/1:128
    BSF ((OPTION_REG) and 07Fh), 1
    BCF ((OPTION_REG) and 07Fh), 0
    reset_tmr0 ; Ejecutar macro que reinicia el TRM0
    RETURN

config_tmr1:
    BANKSEL T1CON ; Cambiar a banco 00
    BSF ((T1CON) and 07Fh), 0 ; Encender TMR1
    BCF ((T1CON) and 07Fh), 1 ; Configurar reloj interno
    BCF ((T1CON) and 07Fh), 3 ; Apagar oscilador LP
    BSF ((T1CON) and 07Fh), 5 ; Configurar prescaler 1:4
    BCF ((T1CON) and 07Fh), 4
    BCF ((T1CON) and 07Fh), 6 ; TRM1 siempre contando
    reset_tmr1 0x85, 0xA3 ; Ejecutar macro que reinicia el TRM1
    RETURN

config_tmr2:
    BANKSEL T2CON
    BSF ((T2CON) and 07Fh), 1 ; Prescaler/11/1:16
    BSF ((T2CON) and 07Fh), 0
    BSF ((T2CON) and 07Fh), 2 ; Encender TRM2
    BSF ((T2CON) and 07Fh), 6 ; Postscaler/1111/1:16
    BSF ((T2CON) and 07Fh), 5
    BSF ((T2CON) and 07Fh), 4
    BSF ((T2CON) and 07Fh), 3
    BANKSEL PR2
    MOVLW 245 ; Mover literal 245 a W y luego al registro PR2
    MOVWF PR2
    RETURN

config_int:
    BANKSEL PIE1 ; Cambiar de banco
    BSF ((PIE1) and 07Fh), 0 ; Habilitar interrupción de TMR1
    BSF ((PIE1) and 07Fh), 1 ; Habilitar interrupción de TRM2
    BANKSEL IOCB ; Cambiar de banco
    BSF IOCB, 0 ; Habilitar interrupción On_change del pin ((PORTB) and 07Fh), 0
    BSF IOCB, 1 ; Habilitar interrupción On_change del pin ((PORTB) and 07Fh), 1
    BSF IOCB, 2 ; Habilitar interrupción On_change del pin ((PORTB) and 07Fh), 2
    BSF IOCB, 3 ; Habilitar interrupción On_change del pin ((PORTB) and 07Fh), 3
    BSF IOCB, 4 ; Habilitar interrupción On_change del pin ((PORTB) and 07Fh), 4
    BANKSEL INTCON ; Cambiar de banco
    BSF ((INTCON) and 07Fh), 7 ; Habilitar interrupciones globales
    BSF ((INTCON) and 07Fh), 6 ; Habilitar interrupciones periféricas
    BSF ((INTCON) and 07Fh), 3 ; Habilitar interrupciones del PORTB
    BSF ((INTCON) and 07Fh), 5 ; Habilitar interrupción de TRM0
    BCF ((INTCON) and 07Fh), 2 ; Limpiar bandera de interrupción del TRM0
    BCF ((PIR1) and 07Fh), 0 ; Limpiar bandera de interrupción del TRM1
    BCF ((PIR1) and 07Fh), 1 ; Limpiar bandera de interrupción del TRM2
    BCF ((INTCON) and 07Fh), 0 ; Limpiar bandera de interrupción del PORTB
    RETURN

