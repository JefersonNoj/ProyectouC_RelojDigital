    MOVF decenasD, 0
    CALL tabla
    MOVWF displayH+1
    RETURN

obtenerDU_SC: ; Ver comentarios para subrutina "obtenerDU_M"
    CLRF decenasSC
    MOVF segundosC, 0
    MOVWF temp1
    MOVF temp1, 0
    MOVWF temp2
    MOVLW 10
    SUBWF temp1, 1
    BTFSS STATUS, 0
    GOTO obtenerU_SC
    MOVF temp1, 0
    MOVWF temp2
    INCF decenasSC
    GOTO $-7
    obtenerU_SC:
 MOVF temp2, 0
 MOVWF unidadesSC
 RETURN

obtenerDU_MC: ; Ver comentarios para subrutina "obtenerDU_M"
    CLRF decenasMC
    MOVF minutosC, 0
    MOVWF temp1
    MOVF temp1, 0
    MOVWF temp2
    MOVLW 10
    SUBWF temp1, 1
    BTFSS STATUS, 0
    GOTO obtenerU_MC
    MOVF temp1, 0
    MOVWF temp2
    INCF decenasMC
    GOTO $-7
    obtenerU_MC:
 MOVF temp2, 0
 MOVWF unidadesMC
 RETURN

config_display_cron: ; Ver comentarios para subrutina "config_display_reloj"
    MOVF unidadesSC, 0
